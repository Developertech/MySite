<!DOCTYPE html>
<html lang="en">
    <head>
        <title>@ViewBag.Title</title>
        <meta charset="utf-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <meta name="description" content="Powered On Dev.Net" />
        <link href="~/Content/font-awesome-4.2.0/css/font-awesome.min.css" rel="stylesheet" />
        @Html.Partial("~/Views/Shared/Sections/_StyleSheet.cshtml")
        @Html.Partial("~/Views/Shared/Sections/_JavaScriptTop.cshtml")
        <script src="http://code.jquery.com/jquery.min.js"></script>
        <script src="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.5/jquery-ui.min.js"></script>
        <script src="http://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.3/jquery.easing.min.js"></script>
        <script src="~/Scripts/jquery.validate.min.js"></script>
    </head>
    <body data-spy="scroll" data-offset="0" data-target="#navbar-main">
        @Html.Partial("~/Views/Shared/Sections/_HeaderPartial.cshtml")
        @RenderBody()
        @Html.Partial("~/Views/Shared/Sections/_FooterPartial.cshtml")
        @Html.Partial("~/Views/Shared/Sections/_JavaScriptBottom.cshtml")
        
        

        @System.Web.Optimization.Scripts.Render("~/Scripts/jquery.validate.min.js")
        @RenderSection("scripts", required: false)

    </body>
</html>